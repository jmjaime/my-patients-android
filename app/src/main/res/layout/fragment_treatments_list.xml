<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
                name="viewModel"
                type="com.jmj.mypatients.views.treatment.TreatmentsViewModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/treatments_list"
            android:name="com.jmj.mypatients.views.treatment.TreatmentsFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            app:layoutManager="LinearLayoutManager"
            tools:context=".views.treatment.TreatmentsFragment"
            tools:listitem="@layout/fragment_treatments" />
</layout>