<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".views.home.HomeFragment" android:id="@+id/frameLayout">
    <Button
            android:text="Session"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/registerSessionsButton"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="80dp"/>
    <Button
            android:text="pay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/registerPatientPayment"
            app:layout_constraintTop_toBottomOf="@+id/initTreatmentButton"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="80dp"
    />
    <Button
            android:text="init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/initTreatmentButton"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="80dp"
    />
    <Button
            android:text="Treatments"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/treatmentsButton"
            app:layout_constraintTop_toBottomOf="@+id/registerSessionsButton"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="80dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>